{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "object",
	"properties": {
		"$schema": {
			"type": "string"
		},
		"content": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"Name": {
						"type": "string",
						"minLength": 1
					},
					"Description": {
						"type": "string",
						"minLength": 1
					},
					"Postpone": {
						"oneOf": [
							{
								"type": "boolean"
							},
							{
								"type": "array",
								"items": {
									"type": "string",
									"minLength": 1
								},
								"minItems": 1
							}
						]
					},
					"APT": {
						"type": "array",
						"items": {
							"type": "string",
							"minLength": 1
						}
					},
					"Chocolatey": {
						"type": "array",
						"items": {
							"type": "string",
							"minLength": 1
						}
					},
					"Homebrew": {
						"type": "array",
						"items": {
							"type": "string",
							"minLength": 1
						}
					},
					"NPM": {
						"type": "array",
						"items": {
							"type": "string",
							"minLength": 1
						}
					},
					"Pipx": {
						"type": "array",
						"items": {
							"type": "string",
							"minLength": 1
						}
					},
					"Env": {
						"type": "array",
						"items": {
							"type": "string",
							"minLength": 1
						}
					},
					"PathLinux": {
						"type": "array",
						"items": {
							"type": "string",
							"minLength": 1
						}
					},
					"PathMacOS": {
						"type": "array",
						"items": {
							"type": "string",
							"minLength": 1
						}
					},
					"PathWindows": {
						"type": "array",
						"items": {
							"type": "string",
							"minLength": 1
						}
					}
				},
				"required": [
					"Name",
					"Description",
					"Postpone",
					"APT",
					"Chocolatey",
					"Homebrew",
					"NPM",
					"Pipx",
					"Env",
					"PathLinux",
					"PathMacOS",
					"PathWindows"
				]
			}
		}
	},
	"required": [
		"$schema",
		"content"
	]
}